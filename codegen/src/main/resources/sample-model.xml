<?xml version="1.0"?>
<model name="my_sample" package="com.sample" dbSchema="sampledb">
  <!-- enum object using microsoft naming convention -->
  <entity name="EntityType" aggregationRoot="false" enum="true" generate="true">
    <field no="1" name="id" type="int" required="true"/>
    <field no="2" name="name" type="string" length="64"/>
    <primaryKey name="EntityTypePk">
      <field name="id"/>
    </primaryKey>
    <row id="0" name="ENUM_CONSTANT_ONE"/>
    <row id="1" name="ENUM_CONSTANT_TWO"/>
    <row id="2" name="ENUM_CONSTANT_THREE"/>
  </entity>
  <!-- enum object using unix c naming convention -->
  <entity name="one_entity" aggregationRoot="true" generate="true">
    <field no="1" name="one_entity_id" type="string" length="64" required="true"/>
    <field no="2" name="one_entity_name" type="string" length="64" aggregation="true"/>
    <field no="3" name="one_entity_type" type="EntityType" aggregation="true"/>
    <field no="4" name="one_entity_desc" type="string" length="64" aggregation="true"/>
    <field no="5" name="one_entity_code" type="string" length="64" aggregation="true"/>
    <field no="6" name="one_entity_abbreviation" type="string" length="64" aggregation="true"/>
    <field no="7" name="one_entity_weight" type="double" aggregation="true"/>
    <message name="wash_brain">
      <field no="1" name="one_entity_id"/>
      <field no="2" name="one_entity_name"/>
    </message>
    <primaryKey name="one_entity_pk">
      <field name="one_entity_id"/>
    </primaryKey>
  </entity>
  <!-- enum object using microsoft naming convention -->
  <entity name="AnotherEntity" aggregationRoot="true" generate="true">
    <field no="1" name="AnotherEntityId" type="string" length="64" required="true"/>
    <field no="2" name="AnotherEntityName" type="string" length="64" aggregation="true"/>
    <field no="3" name="AnotherEntityType" type="EntityType" aggregation="true"/>
    <field no="4" name="AnotherEntityDesc" type="string" length="64" aggregation="true"/>
    <field no="5" name="AnotherEntityCode" type="string" length="64" aggregation="true"/>
    <field no="6" name="AnotherEntityAbbreviation" type="string" length="64" aggregation="true"/>
    <field no="7" name="AnotherEntityWeight" type="double" aggregation="true"/>
    <primaryKey name="AnotherEntityPk">
      <field name="AnotherEntityId"/>
    </primaryKey>
  </entity>
  <!-- parent-child relationship -->
  <entity name="animal_species" enum="true">
    <field no="1" name="id" type="int" required="true"/>
    <field no="2" name="name" type="string" length="64"/>
    <primaryKey name="animal_species_pk">
      <field name="id"/>
    </primaryKey>
    <row id="0" name="FOOL_SPECIES"/>
    <row id="1" name="DOG_SPECIES"/>
    <row id="2" name="CAT_SPECIES"/>
  </entity>
  <entity name="animal" aggregationRoot="true" generate="true">
    <field no="1" name="id" type="int" required="true"/>
    <field no="2" name="name" type="string" length="64" aggregation="true"/>
    <field no="3" name="species" type="animal_species" />
    <field no="4" name="birth_day" type="timestamp" />
    <primaryKey name="animal_pk">
      <field name="id"/>
    </primaryKey>
  </entity>
  <entity name="cat" aggregationRoot="true" generate="true" extends="animal">
    <field no="1" name="cat_id" type="int" required="true"/>
    <field no="4" name="desc" type="string" length="64" aggregation="true"/>
    <primaryKey name="cat_pk">
      <field name="cat_id"/>
    </primaryKey>
    <foreignKey name="cat_animal_fk" refEntity="animal">
      <field name="cat_id" refField="id"/>
    </foreignKey>
  </entity>
  <entity name="dog" aggregationRoot="true" generate="true" extends="animal">
    <field no="1" name="dog_id" type="int" required="true"/>
    <field no="4" name="desc" type="string" length="64" aggregation="true"/>
    <primaryKey name="dog_pk">
      <field name="dog_id"/>
    </primaryKey>
    <foreignKey name="dog_animal_fk" refEntity="animal">
      <field name="dog_id" refField="id"/>
    </foreignKey>
  </entity>
  <entity name="fool" aggregationRoot="true" generate="true" extends="animal">
    <field no="1" name="id" type="int" required="true"/>
    <field no="4" name="bite" type="bool" aggregation="true"/>
    <primaryKey name="fool_pk">
      <field name="id"/>
    </primaryKey>
    <foreignKey name="fool_animal_fk" refEntity="animal">
      <field name="id" refField="id"/>
    </foreignKey>
  </entity>
</model>
